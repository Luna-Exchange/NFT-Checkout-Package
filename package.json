{
    "name": "nft-checkout",
    "version": "1.3.9",
    "description": "Luna NFT plugin",
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/esm/index.d.ts",
    "license": "MIT",
    "scripts": {
        "build": "yarn build:esm && yarn build:cjs",
        "build:esm": "yarn clean && tsc --outDir lib/esm",
        "build:cjs": "tsc --module commonjs --outDir lib/cjs && yarn copy-files",
        "prepublishOnly": "yarn build",
        "clean": "rimraf lib/",
        "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css lib/esm/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Luna-Exchange/NFT-Checkout-package.git"
    },
    "peerDependencies": {},
    "files": [
        "/lib"
    ],
    "dependencies": {
        "@iconify/react": "^3.2.2",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@web3-react/core": "^6.1.9",
        "@web3-react/injected-connector": "^6.0.7",
        "@web3-react/walletconnect-connector": "^6.2.13",
        "@web3-react/walletlink-connector": "^6.2.14",
        "autoprefixer": "^10.4.7",
        "axios": "^0.27.2",
        "copyfiles": "^2.4.1",
        "ethers": "^5.6.9",
        "postcss": "^8.4.14",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-loading-icons": "^1.1.0",
        "rimraf": "^3.0.2",
        "tailwindcss": "^3.1.6",
        "typescript": "^4.1.2"
    }
}
